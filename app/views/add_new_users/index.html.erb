<h1>Listing add_new_users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_new_users.each do |add_new_user| %>
      <tr>
        <td><%= add_new_user.name %></td>
        <td><%= add_new_user.age %></td>
        <td><%= link_to 'Show', add_new_user %></td>
        <td><%= link_to 'Edit', edit_add_new_user_path(add_new_user) %></td>
        <td><%= link_to 'Destroy', add_new_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add new user', new_add_new_user_path %>
